<h1>Editing article</h1>

<% form_for :article, @article, :url => { :action => 'update', :id => @article } do |f| %>
<%= render :partial => 'form', :locals => { :f => f } %>
  <%= submit_tag 'Edit' %>
<% end %>

<%= link_to 'Show', :action => 'show', :id => @article %> |
<%= link_to 'Back', :action => 'list' %>
